   "source": [
    "class Solution:\n",
    "    def titleToNumber(self, s: str) -> int:\n",
    "        ans = 0\n",
    "        while len(s):\n",
    "            ans += (ord(s[0])-64)*((26)**(len(s)-1))\n",
    "            s = s[1:]\n",
    "        return ans\n"
   ]
  }
